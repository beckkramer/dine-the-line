{
  "name": "dine-the-line",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "http://github.com/beckkramer/dine-the-line",
  "author": "beckkramer <kramer.beck@gmail.com>",
  "license": "MIT",
  "scripts": {
    "devclient": "cd client && npm start",
    "devserver": "nodemon server.js",
    "prodclient": "cd client && npm install",
    "prodserver": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run devserver\" \"npm run devclient\"",
    "start": "concurrently --kill-others-on-fail \"npm run prodserver\" \"npm run prodclient\""
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "yelp-fusion": "^2.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.14.12"
  }
}
